import java.util.Scanner;

public class LabProgram {
   public static void main(String[] args) {
         Scanner input = new Scanner(System.in);

         
         int a;
         int b;
         int c;
         
         a = input.nextInt();
         b = input.nextInt();
         c = input.nextInt();
         
         int smallest = a;
            if( b < smallest){
               smallest = b;
            }
            if (c < smallest){
               smallest = c;
            }
            
            System.out.println(smallest);
   }
}