import java.util.Scanner;

public class LabProgram {
   public static void main(String[] args) {
      Scanner scnr = new Scanner(System.in);

   int a;
   int b;
   int c;
   
   a = scnr.nextInt();
   b = scnr.nextInt();
   c = scnr.nextInt();
   
   int l = a;
      if(b > l ){
         l =b;
      }
      if (c > l){
         l = c;
      }
      
      System.out.println(l);
   }
}
